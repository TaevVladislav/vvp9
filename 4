#include <iostream>
#include <algorithm>
using namespace std;
int main()
{
	int a;
	int b;
	int c;
	cin >> a;
	cin >> b;
	cin >> c;
	if (a >= c, b >= c) {
		int a1 = a / c;
		int b1 = b / c;
		int k = max(a1, b1);
		cout << "count = " << k << endl;
		int s = a * b - c * c * k;
		cout << "s = " << s;
	}
}
