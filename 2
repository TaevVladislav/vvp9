#include <iostream>
using namespace std;
int main()
{
	int k;
	cin >> k;
	int n = k % 7;
	cout << n << " day of week" << endl;
	if (n == 1) {
		cout << "monday";
	}
	else if (n == 2) {
		cout << "tuesday";
	}
	else if (n == 3) {
		cout << "wednesday";
	}
	else if (n == 4) {
		cout << "thursday";
	}
	else if (n == 5) {
		cout << "friday";
	}
	else if (n == 6) {
		cout << "saturday";
	}
	else if (n == 0) {
		cout << "sunday";
	}
}
